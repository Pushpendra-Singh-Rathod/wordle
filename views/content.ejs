<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="style.css" />
    <link
      rel="shortcut icon"
      href="images/wordle-favicon.x-icon"
      type="image/x-icon"
    />
    <title data-testid="page-title">Wordle — The New York Times</title>
  </head>
  <body>
    <div class="navbar">
      <div class="nav"></div>
    </div>
    <div class="container">
      <div class="model">
       <div class="row">
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
       </div>
       <div class="row">
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
       </div>
       <div class="row">
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
       </div>
       <div class="row">
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
       </div>
       <div class="row">
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
       </div>
       <div class="row">
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
        <div class="tile"></div>
       </div>

      <div class="keyboard">
        <!-- Row 1 -->
        <div class="key-row">
          <button class="key" data-key="q">q</button>
          <button class="key" data-key="w" >w</button>
          <button class="key" data-key="e">e</button>
          <button class="key" data-key="r">r</button>
          <button class="key" data-key="t">t</button>
          <button class="key" data-key="y">y</button>
          <button class="key" data-key="u">u</button>
          <button class="key" data-key="i">i</button>
          <button class="key" data-key="o">o</button>
          <button class="key" data-key="p">p</button>
        </div>

        <!-- Row 2 -->
        <div class="key-row">
          <div style="flex: 0.5"></div>
          <button class="key" data-key="a">a</button>
          <button class="key" data-key="s">s</button>
          <button class="key" data-key="d">d</button>
          <button class="key" data-key="f">f</button>
          <button class="key" data-key="g">g</button>
          <button class="key" data-key="h">h</button>
          <button class="key" data-key="j">j</button>
          <button class="key" data-key="k">k</button>
          <button class="key" data-key="l">l</button>
          <div style="flex: 0.5"></div>
        </div>

        <!-- Row 3 -->
        <div class="key-row">
          <button class="key wide" data-key="Enter">Enter</button>
          <button class="key" data-key="z">z</button>
          <button class="key" data-key="x">x</button>
          <button class="key" data-key="c">c</button>
          <button class="key" data-key="v">v</button>
          <button class="key" data-key="b">b</button>
          <button class="key" data-key="n">n</button>
          <button class="key" data-key="m">m</button>
          <button class="key wide" data-key="Backspace">←</button>
        </div>
      </div>
    </div>

    <div class="footer">
      <div class="para">
        <p>
          © 2025 The New York Times Compay | NYTimes.com | Sitemap | Privacy
          Policy | Terms of Service | Terms of Sale
        </p>
      </div>
    </div>
    <script type="module" src="script.js"></script>
  </body>
</html>
